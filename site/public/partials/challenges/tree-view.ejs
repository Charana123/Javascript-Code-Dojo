<link rel="stylesheet" href="/css/challenges/tree-view.css" />

<div id="tree-view-container">
    <div id="tree-view-heading"> Challenges </div>
    <ul id="tree-view-list">
      <% data.forEach(function(question){ %>
        <li id="<%= question.id %>" class="challenge-link"> <%= question.title %> </li>
      <% }); %>
    </ul>
</div>
